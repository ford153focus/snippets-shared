# This is the top-most .editorconfig file; do not search in parent directories.
root = true

# All files.
[*]
charset = utf-8
end_of_line = LF
indent_size = 4
indent_style = space
insert_final_newline = true
max_line_length = 150
trim_trailing_whitespace = true

[*.md]
indent_size = 2
trim_trailing_whitespace = false
